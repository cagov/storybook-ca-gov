{
  "name": "storybook-ca-gov",
  "version": "1.0.0",
  "description": "An instance of Storybook intended for developing sharable web components.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "start": "npm run storybook --quiet",
    "build-storybook": "build-storybook",
    "build": "npm run build-storybook",
    "build:commit": "npm run build-storybook && git add storybook-static && git commit -m 'update storybook-static build'",
    "build:data:cagov-reopening": "npm run update:data_activities && npm run update:data_state_guidance && npm run update:data_figma_content_api",
    "update:data_activities": "node -r dotenv/config ./stories/web-components/cagov-reopening/api/scripts/buildAirtableJsonCsv.js dotenv_config_path=.env/.env.local",
    "update:data_state_guidance": "node -r dotenv/config ./stories/web-components/cagov-reopening/api/scripts/buildStateIndustryGuidance.js dotenv_config_path=.env/.env.local",
    "update:data_figma_content_api": "node -r dotenv/config ./stories/web-components/cagov-reopening/api/scripts/buildStateIndustryGuidanceFigma.js dotenv_config_path=.env/.env.local"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-a11y": "^6.2.0-alpha.16",
    "@storybook/addon-actions": "^6.2.0-alpha.16",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-essentials": "^6.2.0-alpha.16",
    "@storybook/addon-links": "^6.2.0-alpha.16",
    "@storybook/addon-storyshots": "^6.2.0-alpha.16",
    "@storybook/addon-viewport": "^6.2.0-alpha.16",
    "@storybook/web-components": "^6.2.0-alpha.16",
    "babel-loader": "^8.2.2",
    "dotenv": "^8.2.0",
    "lit-html": "^1.3.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "storybook-addon-designs": "^5.4.3"
  },
  "dependencies": {
    "@cagov/accordion": "^2.0.0",
    "@cagov/arrow": "^1.0.1",
    "@storybook/preset-scss": "^1.0.3",
    "airtable": "^0.10.1",
    "autoprefixer": "^10.2.3",
    "awesomplete-es6": "^1.2.2",
    "css-loader": "^5.0.1",
    "d3": "^6.3.1",
    "immer": ">=8.0.1",
    "json2csv": "^5.0.5",
    "marked": "^2.0.0",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "ts-dedent": "^2.0.0"
  }
